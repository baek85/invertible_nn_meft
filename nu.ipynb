    "import torch\n",
    "\n",
    "x = torch.tensor(0.5).to(torch.float32)\n",
    "y1 = x * (2**24-1)\n",
    "print(x + y1 - y1 - x)\n",
    "\n",
    "y2 = x * (2**24)\n",
    "print(x + y2 - y2 - x)\n",
    "\n",
    "result = x + y2\n",
    "residual = result - y2 - x\n",
    "print(residual)\n",
    "\n"
